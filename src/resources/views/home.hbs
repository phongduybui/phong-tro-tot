<div class="container">
    <div class="map-search">
        <div class="search-bar-container">
            <div class="search-bar">
                <input class="form-control mr-sm-2 mb25 search-keyword" type="search"
                    placeholder="Search by keyword" aria-label="Search">

                <select id="province" class="custom-select mb25">
                    <option selected>Province/City</option>
                </select>

                <select id="district" class="custom-select mb25">
                    <option selected>District</option>
                </select>

                <select id="ward" class="custom-select mb25">
                    <option selected>Ward</option>
                </select>

                <select id="price" class="custom-select mb25">
                    <option selected>Price (VND)</option>
                    <option value="1">
                        < 1 milion</option>
                    <option value="2">1 - 2 milion</option>
                    <option value="2">2 - 3 milion</option>
                    <option value="2">3 - 5 milion</option>
                    <option value="2">5 - 7 milion </option>
                    <option value="2">7 - 10 milion</option>
                    <option value="2">> 10 milion </option>
                </select>
                <div class="area-range">
                    <div class="area-desc">
                        <i class="fas fa-chart-area"></i>
                        <span class="span-area">Area(m<sup>2</sup>)</span>
                    </div>
                    <div class="range">
                        <div class="sliderValue">
                            <span class="area-span">100</span>
                        </div>
                        <div class="field">
                            <div class="value left">0</div>
                            <input class="area-input" type="range" min="10" max="200" value="100" steps="1">
                            <div class="value right">200</div>
                        </div>
                    </div>
                </div>

                <button type="button" class="btn btn-primary btn-form-search">Search</button>
            </div>
        </div>
        <div class="map-responsive">
            <iframe
                src="https://www.google.com/maps/embed?pb=!1m16!1m12!1m3!1d43378.64499950561!2d108.19472693280031!3d16.056834217620402!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!2m1!1zbmjDoCB0cuG7jQ!5e0!3m2!1svi!2s!4v1604989500052!5m2!1svi!2s"
                width="600" height="450" frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false"
                tabindex="0"></iframe>
        </div>
    </div>
</div>




<div class="container">
    <div class="search-title">
        <h3 class="search-title-h3"><span>News</span>page</h3>
        <p class="search-title-desc">Lorem ipsum dolor sit amet consectetur.</p>
        <div class="line-text">
            <div class="line-text-icon"><i class="fas fa-newspaper"></i></div>
        </div>

